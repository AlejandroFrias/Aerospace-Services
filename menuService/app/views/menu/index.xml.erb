<menu>
<% if @menu %>
  <meals>
  <% @menu.meals.each do |m| %>
    <food>
    	<%= m["name"] %>
    </food>
  <% end %>
  </meals>
  <% @menu.attributes.each do |a| %>
    <% unless a.first == "id" %>
      <<%= a.first %>>
        <%= a.second %>
      </<%= a.first %>>
    <% end %>
  <% end %>
<% end %>
</menu>
