<% @buildings.each_with_index do |b, i| %>
  <poi>
    <tag><%= @tags[i] %></tag>
    <% b.attributes.each do |a| %>
      <% unless a.first == "id" %>
        <<%= a.first %>>
          <%= a.second %>
        </<%= a.first %>>
      <% end %>
    <% end %>
    <update_url>
     <%  up = building_url(b.attributes["id"]) %>
     <%  up["localhost"] = @SERVER_IP unless up["localhost"].nil? %>
     <%= up %>
    </update_url>
    <update_freq>
      10
    </update_freq>
  </poi>
<% end %>

