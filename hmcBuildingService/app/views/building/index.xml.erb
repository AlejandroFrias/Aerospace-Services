<% @buildings.each do |b| %>
  <poi>
    <% b.attributes.each do |a| %>
      <<%= a.first %>>
        <%= a.second %>
      </<%= a.first %>>
    <% end %>
    <update_url>
      <%= building_url(b.attributes["id"]) %>
    </update_url>
    <update_freq>
      10
    </update_freq>
  </poi>
<% end %>

