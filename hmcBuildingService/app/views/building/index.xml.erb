<% @buildings.each do |b| %>
  <poi>
    <tag><%= @tags[b.attributes["id"]] %></tag>
    <% b.attributes.each do |a| %>
      <% unless a.first == "id" %>
        <<%= a.first %>>
          <%= a.second %>
        </<%= a.first %>>
      <% end %>
    <% end %>
    <update_url>
     134.173.43.11:3000<%= building_path(b.attributes["id"]) %>
    </update_url>
    <update_freq>
      10
    </update_freq>
  </poi>
<% end %>

